{
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "prettier"
    ]
}
