    "html-webpack-plugin": "3.2.0",
    "prettier": "1.14.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "reshake": "1.1.5",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
