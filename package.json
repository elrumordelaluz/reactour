    "intro",
    "guide",
    "introduction",
    "step-by-step",
    "guide",
    "walkthrough",
    "joyride",
    "tourist-guide",
    "product-intro",
    "presentation"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Lionel T <elrumordelaluz@hotmail.com> (https://elrumordelaluz.com/)",
  "license": "MIT",
